{
  "name": "deepfence_frontend",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "test": "turbo run test -- --run",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier --check \"./**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:fix": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\""
  },
  "devDependencies": {
    "prettier": "^2.8.8",
    "turbo": "latest",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@7.17.1"
}
