    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "msw": "^2.4.5",
    "postcss": "^8.4.45",
    "resize-observer-polyfill": "^1.5.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.6.2",
    "uuid": "^9.0.1",
    "vite": "^5.4.4",
    "vite-plugin-webfont-dl": "^3.9.5",
    "vitest": "^1.6.0",
